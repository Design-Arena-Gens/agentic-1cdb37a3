# Optional: Add your Anthropic API key for AI-powered content generation
# Without this, the system will use high-quality fallback content
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Add OpenAI API key for actual image generation
# Without this, only image prompts will be generated
OPENAI_API_KEY=your_openai_api_key_here

# Optional: AWS credentials for S3 storage
# Without these, mock S3 URLs will be returned
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-bucket-name
